{
  "name": "@opcgdb/types",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/types",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/types",
        "commands": ["rimraf dist", "tsc"]
      }
    },
    "watch": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/types",
        "commands": ["rimraf dist", "tsc --watch"]
      }
    }
  }
}
