{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "serve"]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"]
